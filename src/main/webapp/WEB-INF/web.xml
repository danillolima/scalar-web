<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	 version="3.1">
  
    
    <servlet>
             <servlet-name>cadastro</servlet-name>
             <jsp-file>WEB-INF/cadastro.jsp</jsp-file>
    </servlet>
      
    <servlet-mapping>
              <servlet-name>cadastro</servlet-name>
              <url-pattern>/cadastro</url-pattern>
    </servlet-mapping>
    
       <servlet>
             <servlet-name>login</servlet-name>
             <jsp-file>WEB-INF/login.jsp</jsp-file>
    </servlet>
      
    <servlet-mapping>
              <servlet-name>login</servlet-name>
              <url-pattern>/login</url-pattern>
    </servlet-mapping>
    
      <servlet>
             <servlet-name>profile</servlet-name>
             <jsp-file>WEB-INF/profile.jsp</jsp-file>
    </servlet>
      
    <servlet-mapping>
              <servlet-name>profile</servlet-name>
              <url-pattern>/profile/*</url-pattern>
        
    </servlet-mapping>
    
    <servlet>
             <servlet-name>publicar</servlet-name>
             <jsp-file>WEB-INF/publicar.jsp</jsp-file>
    </servlet>
      
    <servlet-mapping>
              <servlet-name>publicar</servlet-name>
              <url-pattern>/publicar</url-pattern>
    </servlet-mapping>
    
    
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
